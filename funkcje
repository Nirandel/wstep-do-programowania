#include<stdio.h> int qq(n,k) {if(n==0) return 1; else if(n!=0 && k==0) return qq(n-1,5); else return qq(n,k-1)+qq(n-1,2*k);} int main () { int k,n; printf("Podaj liczbe n: "); scanf("%i", &n); printf("Podaj liczbe k: "); scanf("%i", &k); if((n<0) || (k<0)) printf("Obie liczby musza byc wieksze lub rowne 0 !!!\n"); else printf("Wynik to: %i\n",qq(n,k)); return 0; }
